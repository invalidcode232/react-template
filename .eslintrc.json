{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "plugin:react/recommended",
        "plugin:react/jsx-runtime"
    ],
    "rules": {
        "semi": ["warn", "always"],
        "quotes": ["warn", "single"],
        "no-duplicate-imports": "warn",
        "@typescript-eslint/ban-types": "off"
    }
}
